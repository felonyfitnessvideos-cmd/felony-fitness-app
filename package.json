{
  "name": "felony-fitness-app",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "preview": "vite preview",
    "prepare": "husky",
    "precommit": "lint-staged",
    "seed:foods": "node scripts/bulk-food-import.js",
    "seed:foods:quick": "node scripts/direct-food-seeder.js",
    "seed:foods:test": "node scripts/bulk-food-import.js --categories Proteins --max-per-category 5",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tinymce/tinymce-react": "^6.3.0",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.544.0",
    "node-fetch": "^3.3.2",
    "posthog-js": "^1.295.0",
    "react": "^18.3.1",
    "react-body-highlighter": "^2.0.5",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^4.0.5",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.7.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "supabase": "^2.51.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^4.0.1"
  }
}
